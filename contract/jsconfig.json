// This file can contain .js-specific Typescript compiler config.
{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES2022",

    "noEmit": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.js", "test/**/*.js", "exported.js", "globals.d.ts"]
}
